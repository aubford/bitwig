{
    "extends": "eslint:recommended",
    "rules": {
        "eqeqeq": 2,
        "max-len": [2, {"code": 145}],
        "quotes": [2, "single"],
        "no-mixed-spaces-and-tabs": [2],
        "linebreak-style": [2, "unix"],
        "semi": [2, "always"],
        "no-cond-assign": [2, "always"],
        "array-bracket-spacing": [2, "never"],
        "camelcase": [2, {"properties": "always"}],
        "no-trailing-spaces": 0,
        "no-unused-expressions": 0,
        "no-console": 2,
        "no-unused-vars": 0,
        "eol-last": 0
    }
}